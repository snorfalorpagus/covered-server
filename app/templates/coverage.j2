{% extends "base.j2" %}

{% block head %}
{{ super() }}
<link rel="stylesheet" type="text/css" href="{{ url_for("static", filename="pygments.css") }}">
{% endblock %}

{% block content %}
<h1>{{ path }}</h1>

<p>
Coverage: {{ "%.0f"|format(summary["coverage"]) }} %<br />
{{ summary["hit"] }} run, {{ summary["missing"] }} missing, {{ summary["excluded"] }} excluded
</p>

{{ coverage_table }}
{% endblock %}
